{
    "name": "hiraeth/stash-session",
    "description": "Cache based session storage via stash for the Hiraeth Nano Framework",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name":  "Matthew J. Sahagian",
            "email": "matthew.sahagian@gmail.com",
            "role":  "Lead Architect / Developer"
        }
    ],
    "require": {
        "hiraeth/stash": "^1.0",
        "cache/session-handler": "^1.0"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "type": "opus-package",
    "extra": {
        "opus": {
            "hiraeth/app": {
                "config": ""
            }
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.19"
    }
}
